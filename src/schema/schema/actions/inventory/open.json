{
  "name": "inventory_open",
  "description": "インベントリを閉きます。",
  "base": "inventory_$base",
  "events": [
    "org.bukkit.event.inventory.InventoryOpenEvent"
  ],

  "executable": "プレイヤにインベントリを開かせます。",
  "watchable": "プレイヤがインベントリを開くことを期待します。",
  "requireable": "プレイヤにインベントリを開かせることができます。",

  "arguments": {
    "target": {
      "type": "playerSpecifier",
      "description": "対象のプレイヤです。"
    },
    "inventory": {
      "description": "開くインベントリです。",
      "type": "inventory"
    }
  },
  "outputs": {
    "target": {
      "type": "player",
      "description": "開いたプレイヤです。"
    },
    "inventory": {
      "description": "開いたインベントリです。",
      "type": "inventory"
    }
  },
}
