{
  "name": "entity_spawn",
  "description": "エンティティをスポーンさせます。",
  "base": "entity_$base",
  "events": [
    "org.bukkit.event.entity.EntitySpawnEvent"
  ],

  "executable": "エンティティをスポーンさせます。",
  "watchable": "エンティティがすぽーんすることを期待します。",
  "requireable": false,

  "arguments": {
    "entity": {
      "type": "entity",
      "requiredOn": ["execute"]
    }
  },
  "outputs": {
    "entity": {
      "type": "entity"
    }
  }
}
