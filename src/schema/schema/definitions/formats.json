{
  "version": {
    "type": "string",
    "pattern": "^v?(?<major>[0-9]\\d*)\\.(?<minor>0|[1-9]\\d*)(\\.(?<patch>0|[1-9]\\d*))?(-(?<preRelease>(?:0|[1-9]\\d*|\\d*[a-zA-Z-][0-9a-zA-Z-]*)(?:\\.(?:0|[1-9]\\d*|\\d*[a-zA-Z-][0-9a-zA-Z-]*))*))?(?:\\+(?<buildMetadata>[0-9a-zA-Z-]+(?:\\.[0-9a-zA-Z-]+)*))?$",
    "examples": [
      "v3.0.0",
      "3.0.0-alpha.1",
      "v3.0.0-alpha.1+build.1",
      "3.0.0+build.1",
      "v3.0.0+build.1.2.b8f12d7",
      "3.0.0-beta.2",
      "v3.0.0-beta.11",
      "3.0.0-rc.1",
      "v3.0.0-rc.1+build.1",
      "3.0.0-rc.1+build.1.2.b8f12d7"
    ]
  },
  "namespacedKey": {
    "type": "string",
    "pattern": "^([a-z0-9_/]+:)?[a-z0-9_/]+$",
    "description": "名前空間付きキーです。 名前空間を指定しない場合は、minecraft: が暗黙的に指定されます。",
    "examples": [
      "minecraft:stone",
      "stone"
    ]
  },
  "selector": {
    "type": "string",
    "pattern": "^(@[eaprs])?(\\[(([^\"[\\]=!,{}]+)=!?([^\"\\],]+|\"[^\"\\],]+\"|{(([^\",{}]|(([^\"[\\]=!,{}]+)=!?([^\"\\],]+|\"[^\"\\],]+\"|{(([^\",{}]|(\\[(([^\"[\\]=!,{}]+)=!?([^\"\\],]+|\"[^\"\\],]+\"|{(([^\",{}]|(([^\"[\\]=!,{}]+)=!?([^\"\\],]+|\"[^\"\\],]+\"|{(([^\",{}]|(\\[(([^\"[\\]=!,{}]+)=!?([^\"\\],]+|\"[^\"\\],]+\"|{(([^\",{}]|(([^\"[\\]=!,{}]+)=!?([^\"\\],]+|\"[^\"\\],]+\"|{(([^\",{}]|(\\[(([^\"[\\]=!,{}]+)=!?([^\"\\],]+|\"[^\"\\],]+\"|{(([^\",{}]|(([^\"[\\]=!,{}]+)=!?([^\"\\],]+|\"[^\"\\],]+\"|{(([^\",{}]|(\\[(([^\"[\\]=!,{}]+)=!?([^\"\\],]+|\"[^\"\\],]+\"|{(([^\",{}]|(([^\"[\\]=!,{}]+)=!?([^\"\\],]+|\"[^\"\\],]+\"|{(([^\",{}]|),?)+}),?)+),?)+}),?)+])),?)+}),?)+),?)+}),?)+])),?)+}),?)+),?)+}),?)+])),?)+}),?)+),?)+}),?)+])),?)+}),?)+),?)+}),?)+])?$",
    "description": "セレクターです。",
    "examples": [
      "@e",
      "@e[type=armor_stand]",
      "@e[type=armor_stand,tags={foo,bar}]",
      "@e[type=armor_stand,foo=!{bar=1,baz=2}]"
    ]
  },
  "entitySpecifier": {
    "description": "エンティティを指定するための指定子です。",
    "anyOf": [
      {
        "type": "selector",
        "description": "セレクタを指定します。"
      },
      {
        "type": "entity",
        "description": "エンティティを指定します。"
      },
      {
        "type": "string",
        "format": "uuid",
        "description": "エンティティの UUID を指定します。"
      }
    ]
  },
  "playerSpecifier": {
    "description": "プレイヤを指定するための指定子です。",
    "anyOf": [
      {
        "type": "selector",
        "description": "セレクタを指定します。"
      },
      {
        "type": "player",
        "description": "プレイヤを指定します。"
      },
      {
        "type": "string",
        "format": "uuid",
        "description": "プレイヤの UUID を指定します。"
      },
      {
        "type": "string",
        "description": "プレイヤの名前を指定します。"
      }
    ]
  }
}
