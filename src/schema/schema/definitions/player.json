{
  "itemSlot": {
    "type": "string",
    "enum": [
      "HAND",
      "OFF_HAND",
      "FEET",
      "LEGS",
      "CHEST",
      "HEAD",
      "hand",
      "off_hand",
      "feet",
      "legs",
      "chest",
      "head"
    ]
  },
  "playerInventory": {
    "description": "プレイヤのインベントリを表します。",
    "properties": {
      "main": {
        "type": "inventory",
        "description": "インベントリのメインの部分です。"
      },
      "mainHandItem": {
        "type": "itemStack",
        "description": "インベントリのメインハンドのアイテムです。"
      },
      "offHandItem": {
        "type": "itemStack",
        "description": "インベントリのオフハンドのアイテムです。"
      },
      "armors": {
        "type": "array",
        "description": "インベントリのアーマーのアイテムです。上から順にヘルメット、チェストプレート、レギンス、ブーツです。どこかが空いている場合は null を格納します。",
        "items": {
          "anyOf": [
            {
              "type": "null"
            },
            {
              "type": "itemStack"
            }
          ]
        },
        "minItems": 4,
        "maxItems": 4
      }
    }
  },
  "player": {
    "type": "entity",
    "properties": {
      "name": {
        "type": "string",
        "description": "プレイヤの名前です。",
        "maxLength": 16
      },
      "online": {
        "type": "boolean",
        "description": "プレイヤがオンラインかどうかです。",
        "default": true
      },
      "displayName": {
        "type": "string",
        "description": "プレイヤの表示名です。"
      },
      "playerList": {
        "type": "object",
        "description": "プレイヤのプレイヤリストにおける扱いです。",
        "properties": {
          "name": {
            "type": "string",
            "description": "プレイヤリストに表示される名前です。"
          },
          "header": {
            "type": "string",
            "description": "プレイヤリストに表示されるヘッダーです。"
          },
          "footer": {
            "type": "string",
            "description": "プレイヤリストに表示されるフッターです。"
          }
        }
      },
      "compass": {
        "type": "location",
        "description": "プレイヤのコンパスのターゲットです。"
      },
      "bedLocation": {
        "type": "location",
        "description": "プレイヤのベッドスポーンの位置です。"
      },
      "exp": {
        "type": "integer",
        "description": "プレイヤの経験値です。"
      },
      "level": {
        "type": "integer",
        "description": "プレイヤのレベルです。"
      },
      "totalExp": {
        "type": "integer",
        "description": "プレイヤの総経験値です。"
      },
      "flyable": {
        "type": "boolean",
        "description": "プレイヤが飛行できるかどうかです。ゲームモードがクリエイティブおよびスペクテイタの場合は、暗黙的に true になります。"
      },
      "flying": {
        "type": "boolean",
        "description": "プレイヤが飛行中かどうかです。"
      },
      "walkSpeed": {
        "type": "number",
        "description": "プレイヤの歩行速度です。"
      },
      "flySpeed": {
        "type": "number",
        "description": "プレイヤの飛行速度です。"
      },
      "inventory": {
        "type": "playerInventory",
        "description": "プレイヤのインベントリです。"
      },
      "enderChest": {
        "type": "inventory",
        "description": "プレイヤのエンダーチェストです。"
      },
      "mainHand": {
        "type": "string",
        "description": "プレイヤの利き手です。",
        "enum": [
          "left",
          "right",
          "LEFT",
          "RIGHT"
        ],
        "default": "right"
      },
      "gamemode": {
        "type": "string",
        "description": "プレイヤのゲームモードです。",
        "enum": [
          "survival",
          "creative",
          "adventure",
          "spectator",
          "SURVIVAL",
          "CREATIVE",
          "ADVENTURE",
          "SPECTATOR"
        ],
        "default": "survival"
      },
      "foodLevel": {
        "type": "integer",
        "description": "プレイヤの食料レベルです。",
        "minimum": 0,
        "maximum": 20,
        "default": 20
      },
      "op": {
        "type": [
          "boolean",
          "integer"
        ],
        "minimum": 0,
        "maximum": 4,
        "description": "プレイヤの OP レベルです。 0 は OP ではないことを意味します。"
      },
      "permissions": {
        "type": "array",
        "description": "プレイヤに与える権限です。",
        "items": {
          "type": "string"
        }
      }
    },
    "required": [
      "name"
    ]
  },
  "teleportCause": {
    "type": "string",
    "enum": [
      "COMMAND",
      "DEATH",
      "END_PORTAL",
      "END_GATEWAY",
      "CHORUS_FRUIT",
      "SPECTATE",
      "NETHER_PORTAL",
      "ENDER_PEARL",
      "UNKNOWN"
    ]
  }
}
